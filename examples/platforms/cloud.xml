        <link_ctn id="link_dc3_cb1"/><link_ctn id="link_dc3_cb2"/>
     </ASroute>
     <ASroute src="AS3_cb4" dst="AS3_cb3" gw_src="cb4-AS3_cb4_router.dc3.acloud.com"
